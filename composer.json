{
  "name": "cbwar/laravel-modelchanges",
  "description": "Model changes tracking for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Raphael Lisch",
      "email": "raphael.lisch@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cbwar\\Laravel\\ModelChanges\\": "src",
      "Tests\\": "tests"
    }
  },
  "require": {
    "php": ">=5.5.9",
    "cbwar/php-text-difference": "^1.0",
    "illuminate/support": "5.4.* || 5.5.*",
    "illuminate/database": "5.4.* || 5.5.*",
    "doctrine/dbal": "^2.5"
  },
  "require-dev": {
    "ext-sqlite3": "*",
    "phpunit/phpunit": "^5.2",
    "orchestra/testbench": "3.4.*",
    "orchestra/database": "3.4.*",
    "phpunit/php-code-coverage": "^4.0",
    "squizlabs/php_codesniffer": "^3.0",
    "friendsofphp/php-cs-fixer": "^2.5"
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cbwar\\Laravel\\ModelChanges\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
